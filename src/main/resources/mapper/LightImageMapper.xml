<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lightingshop.dao.LightImageDao">
    
    <select id="listLightImage" parameterType="java.lang.Integer" resultType="LightImage">
        SELECT
            *
        FROM
            lightimages
        WHERE
            lightid = #{id}
    </select>

</mapper>